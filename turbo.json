{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": [
    "RABBITMQ_URI",
    "ENV",
    "PORT",
    "HOSTNAME",
    "AXIOM_DATASET",
    "AXIOM_TOKEN",
    "REDIS_HOST",
    "REDIS_PASSWORD",
    "REDIS_PORT"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": []
    },
    "build:game-client": {},
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "dependsOn": [],
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["^start"]
    },
    "api:migrate:dev": {
      "cache": false,
      "interactive": true
    },
    "api:generate": {
      "cache": false
    },
    "api:studio": {
      "cache": false,
      "interactive": false
    },
    "auth:migrate:dev": {
      "cache": false,
      "interactive": true
    },
    "auth:migrate:prod": {
      "cache": false,
      "interactive": false
    },
    "configure:env": {
      "cache": false,
      "interactive": true
    }
  }
}
